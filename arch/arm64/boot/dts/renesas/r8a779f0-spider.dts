/dts-v1/;
#include "r8a779f0-spider-cpu.dtsi"
#include "r8a779f0-spider-ethernet.dtsi"

/ {
	model = "Renesas Spider CPU and Breakout boards based on r8a779f0";
	compatible = "renesas,spider-breakout", "renesas,spider-cpu", "renesas,r8a779f0";

	aliases {
		eth0 = &rswitch;
        };
};

&i2c4 {
	eeprom@51 {
		compatible = "rohm,br24g01", "atmel,24c01";
		label = "breakout-board";
		reg = <0x51>;
		pagesize = <8>;
	};
};

&pfc {
	tsn0_pins: tsn0 {
		mux {
			groups = "tsn0_link", "tsn0_mdio";
			function = "tsn0";
			drive-strength = <12>;
		};

		pins_mdio {
			groups = "tsn0_mdio";
			drive-strength = <12>;
		};
	};
};

&rswitch {
	status = "okay";

	ports {
		#address-cells = <1>;
		#size-cells = <0>;
		port@0 {
			pinctrl-0 = <&tsn0_pins>;
			pinctrl-names = "default";
			/* phy-mode = "rgmii-txid"; */
			reg = <0>;
			phy-handle = <&etha0>;
			#address-cells = <1>;
			#size-cells = <0>;
			etha0: ethernet-phy@1 {
				reg = <1>;
			};
		};
	};
};
